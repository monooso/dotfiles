[
  // Navigate between panes as if they're splits.
  {
    "context": "Dock",
    "bindings": {
      "ctrl-w h": [
        "workspace::ActivatePaneInDirection",
        "Left"
      ],
      "ctrl-w l": [
        "workspace::ActivatePaneInDirection",
        "Right"
      ],
      "ctrl-w k": [
        "workspace::ActivatePaneInDirection",
        "Up"
      ],
      "ctrl-w j": [
        "workspace::ActivatePaneInDirection",
        "Down"
      ]
    }
  }
]
