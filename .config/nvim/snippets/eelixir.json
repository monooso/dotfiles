{
  "%": {
    "prefix": "%",
    "body": "<% $0 %>"
  },
  "=": {
    "prefix": "=",
    "body": "<%= $0 %>"
  },
  "gettext": {
    "prefix": "gt",
    "body": "<%= gettext(\"$0\") %>"
  },
  "for": {
    "prefix": ":for",
    "body": ":for={${1:item} <- ${2:items}}"
  },
  "if": {
    "prefix": ":if",
    "body": ":if={${1}}"
  }
}
